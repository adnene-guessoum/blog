{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "nginx",
			"image": "public.ecr.aws/y9i0i5l9/blog-nginx-container",
			"hostname": "nginx",
			"essential": true,
			"portMappings":[
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["frontend", "backend", "database"],
			"memory": 100
		},
		{
			"name": "frontend",
			"image": "public.ecr.aws/y9i0i5l9/blog-frontend-container",
			"hostname": "frontend",
			"essential": false,
			"memory": 300
		},
		{
			"name": "backend",
			"image": "public.ecr.aws/y9i0i5l9/blog-backend-container",
			"essential": false,
			"memory": 500
		}
	]
}
