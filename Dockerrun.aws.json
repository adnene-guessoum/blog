{
	"AWSEBDockerrunVersion": 3,
	"containerDefinitions": [
		{
			"name":"nginx",
			"image": "public.ecr.aws/y9i0i5l9/blog-nginx-container",
			"hostname": "nginx",
			"essential": true,
			"portMappings":[
				{
					"hostPort": 80,
					"containerPort":80
				}
			],
			"links": ["frontend", "backend"],
			"memory": 256
		},
		{
			"name": "backend",
			"image": "public.ecr.aws/y9i0i5l9/blog-backend-container",
			"hostname": "backend",
			"essential": true,
			"memory": 512
		},
		{
			"name": "frontend",
			"image": "public.ecr.aws/y9i0i5l9/blog-frontend-container",
			"hostname": "frontend",
			"essential": false,
			"memory": 512
		}
	]
}
